{
  "name": "zoeken-persoon",
  "version": "1.0.0",
  "author": "Bram Janssens",
  "scripts": {
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "clean:dist": "npm run rimraf -- dist",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "rimraf": "rimraf",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "start": "npm run server:dev",
    "test": "npm run lint && karma start",
    "tslint": "tslint",
    "version": "npm run build:prod",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@angular/core": "~2.4.9",
    "@angular/http": "~2.4.9",
    "@angular/platform-browser": "~2.4.9",
    "angular2-cookie": "^1.2.6",
    "anva-core": "*",
    "anva-contracts": "*",
    "anva-ui-components": "*",
    "core-js": "^2.4.1"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.9",
    "@angularclass/hmr": "~1.2.2",
    "@angularclass/hmr-loader": "~3.0.2",
    "@types/hammerjs": "^2.0.34",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "~3.0.8",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "~2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "ng-router-loader": "^2.1.0",
    "ngc-webpack": "1.2.0",
    "node-sass": "^4.5.0",
    "raw-loader": "0.5.1",
    "sass-loader": "^6.0.2",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "style-loader": "^0.13.2",
    "ts-helpers": "1.1.2",
    "typescript": "~2.2.1",
    "url-loader": "^0.5.8",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "~3.0.0"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}